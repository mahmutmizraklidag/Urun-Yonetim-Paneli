@model IEnumerable<Brand>
@{


    var brand = Model;
    <li class="dropdown">
        <a href="#"><span>Markalar</span> <i class="bi bi-chevron-down"></i></a>
        <ul>
            @foreach (var downmenu in brand)
            {
                <li><a href="/Categories/Index/@downmenu.Id">@downmenu.Name</a></li>
            }
        </ul>
    </li>

}